<%= render 'welcome/nav' %>

<%= form_for :login do |f| %>
  <div class="form-group">
    <%#= f.label :email %>
    <%= f.text_field :email, :placeholder => "E-mail", class:"form-control" %>
 
  </div>
  <div class="form-group">
    <%#= f.label :password %>
    <%= f.password_field :password, :placeholder => "Password", class:"form-control" %>

  </div>
  
  <div>
<%=  f.submit 'Log In', :class =>'btn btn-lg btn-block' %>
  </div>

  
<% end %>

